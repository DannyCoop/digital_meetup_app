<h1>Editing User</h1>

<%= form_for @user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :password %>
<%= f.password_field :password %><br>
<%= f.label :user_img_url %>
<%= f.text_field :user_img_url %>
<%= f.label :bio %>
<%= f.text_area :bio %><br>
<%= f.submit %>
<% end %>

<%= link_to 'Show', @user %> |
<%= link_to 'Back', users_path %>
